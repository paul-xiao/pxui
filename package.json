{
  "name": "pxui",
  "version": "0.1.1",
  "private": false,
  "description": "a vue2.0 UI lib",
  "author": "paulxiao",
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve",
    "inspect": "vue inspect > output.js",
    "lib": "vue-cli-service build --target lib --name pxui --dest lib ./src/index.js",
    "wc": "vue-cli-service build --target wc --name px 'packages/**/*.vue'"
  },
  "main": "lib/pxui.umd.min.js",
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "style-resources-loader": "^1.3.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-style-resources-loader": "^0.1.4",
    "vue-template-compiler": "^2.6.11"
  },
  "homepage": "https://github.com/paul-xiao/pxui",
  "keywords": [
    "pxui"
  ],
  "license": "MIT",
  "repository": "git@github.com:paul-xiao/pxui.git"
}
